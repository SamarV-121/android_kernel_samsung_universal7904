/dts-v1/;
/plugin/;

#include "exynos7904-m30_common.dtsi"
#include "exynos7904-m30_chn_open_gpio_00.dtsi"
#include "exynos7904-m30_fingerprint-sensor_00.dtsi"

/ {
	compatible = "samsung,M30 CHN OPEN 00", "samsung,EXYNOS7904";

	dtbo-hw_rev = <0>;
    dtbo-hw_rev_end = <255>;


	fragment@model {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;

			model = "Samsung M30 CHN OPEN 00 board based on EXYNOS7904";

		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

	usb_hs_tune:hs_tune {
		status = "disabled";
		/* value = <device host> */
		tx_vref = <0x3 0x3>;
		tx_pre_emp = <0x0 0x0>;
		tx_pre_emp_puls = <0x0 0x0>;
		tx_res	= <0x2 0x2>;
		tx_rise = <0x1 0x1>;
		tx_hsxv = <0x0 0x0>;
		tx_fsls = <0x3 0x3>;
		rx_sqrx = <0x5 0x5>;
		compdis	= <0x3 0x3>;
		otg= <0x2 0x2>;
		/* true : 1, false: 0 */
		/* <enable_user_imp user_imp_value> */
		enable_user_imp = <0 0>;
		/* PHY clk : 1 , FREE clk : 0 */
		is_phyclock = <1>;
	};

	usb_ss_tune:ss_tune {
		status = "disabled";
		/* value = <device host> */
		tx_boost_level = <0x4 0x4>;
		tx_swing_level = <0x1 0x1>;
		tx_swing_full = <0x7F 0x7F>;
		tx_swing_low = <0x7F 0x7F>;
		tx_deemphasis_mode = <0x1 0x1>;
		tx_deemphasis_3p5db = <0x18 0x18>;
		tx_deemphasis_6db = <0x18 0x18>;
		enable_ssc = <0x1 0x1>;
		ssc_range = <0x0 0x1>;
		los_bias = <0x5 0x5>;
		los_mask_val = <0x104 0x104>;
		enable_fixed_rxeq_mode = <0x0 0x0>;
		fix_rxeq_value = <0x4 0x4>;
